{
  "homepage": "https://roger-95s.github.io/CyberMag/",
  "name": "cybermag",
  "license": "SEE LICENSE  IN LICENSE",
  "description": "A modern, responsive web application for managing and visualizing cybersecurity data.",
  "keywords": [
    "cybersecurity",
    "data visualization",
    "react",
    "web application",
    "responsive design",
    "modern UI",
    "tailwindcss",
    "lucide-react",
    "gh-pages"
  ],
  "author": "Roger95s",
  "contributors": [
    {
      "name": "Roger95s",
      "email": "rogeresleitercc@gmail.com",
      "url": "https://www.linkedin.com/in/roger-campo-cordova-574bb930a"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Roger95s/CyberMag"
  },
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^8.57.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^6.3.0",
    "jest": "^27.5.1",
    "jest-matchmedia-mock": "^1.1.0",
    "postcss": "^8.5.6",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "lucide-react": "^0.525.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^6.18.0",
    "web-vitals": "^5.0.3"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "react-app/jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
